  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Xplorer
  pod 'Alamofire', '~> 4.7'
  pod 'ObjectMapper', '~> 3.4.2'
  pod 'AlamofireObjectMapper', '~> 5.2'

end
